{
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "Microsoft.EntityFrameworkCore": "Warning"
            }
        },
        "ApplicationInsights": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },
    "AllowedHosts": "*",
    "LettuceEncrypt": {
        "AcceptTermsOfService": true,
        "DomainNames": [ "mihubot.xyz" ],
        "EmailAddress": "mihazupan.zupan1@gmail.com"
    },
    "ReverseProxy": {
        "Routes": {
            "corerun-db": {
                "ClusterId": "superpmi",
                "Match": {
                    "Path": "/superpmi/{**catch-all}"
                }
            },
            "mihubot-storage": {
                "ClusterId": "mihubot-storage",
                "AuthorizationPolicy": "Admin",
                "Match": {
                    "Path": "/manage/storage/{**remainder}"
                },
                "Transforms": [
                    {
                        "PathPattern": "/{**remainder}"
                    }
                ]
            },
            "mihubot-storage-update": {
                "ClusterId": "mihubot-storage",
                "Match": {
                    "Path": "/manage/storage/deploy-update"
                },
                "Transforms": [
                    {
                        "PathSet": "/Management/Deployed"
                    }
                ]
            }
        },
        "Clusters": {
            "superpmi": {
                "Destinations": {
                    "azureBlob": {
                        "Address": "https://clrjit2.blob.core.windows.net"
                    }
                }
            },
            "mihubot-storage": {
                "Destinations": {
                    "mihubot-storage": {
                        "Address": "http://mihubot-storage.tunnel"
                    }
                }
            }
        }
    }
}
